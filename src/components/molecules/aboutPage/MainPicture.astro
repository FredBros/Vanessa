---
import { Picture } from "@astrojs/image/components";
import type {Image} from "@/types/types"
type Props={
    image:Image
}

const {image} = Astro.props
---
<div class="rounded-full overflow-hidden h-60 w-60 md:float-right mx-auto">
  <!-- @ts-ignore -->
<Picture
class="object-cover h-full w-full"
        src={image.url}
        widths={[200, 400, 800, 1500]}
        sizes="300px"
        aspectRatio={image.width / image.height}
        alt="image de pratique de medecine chinoise"
      />
</div>