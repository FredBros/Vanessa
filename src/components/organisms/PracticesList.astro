---
import PracticeItem from "./PracticeItem.astro";
import type { Practice, Image } from "@/types/types"; 
type Props = {
    data: Practice[]
}
const {data}= Astro.props

---
<section class="flex flex-wrap gap-4 m-x-auto justify-center">
   {
    data.map((practice)=>(
    <PracticeItem practice={practice}/>
    ))
   }
</section>